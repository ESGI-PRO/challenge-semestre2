// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TypeExercices {
  id Int @id @default(autoincrement())
  name String
  exercices Exercices[]
}

model Exercices {
  id String @id @default(uuid())
  name String 
  type String
  equipment String 
  difficulty String 
  instructions String
  muscle TypeExercices? @relation(fields: [TypeExercicesId], references: [id])
  TypeExercicesId Int?
}